{
  "name": "bot",
  "version": "1.0.0",
  "private": "true",
  "description": "A closed-source Discord bot to connect the GateKeeper platform to Discord.",
  "main": "build/index.js",
  "scripts": {
    "dev:env": "pnpm run build && pnpm run start:env",
    "start": "node .",
    "start:env": "node --env-file .env .",
    "build": "tsc",
    "copy:locales": "copyfiles --error --up 1 ./src/i18n/locales/**/*.* ./build"
  },
  "author": "amory_",
  "engines": {
    "node": ">=20.6.0"
  },
  "dependencies": {
    "@discordjs/rest": "^2.2.0",
    "@t3-oss/env-core": "^0.8.0",
    "axios": "^1.6.8",
    "axios-rate-limit": "^1.3.0",
    "chalk": "^4.1.2",
    "discord.js": "^14.11.0",
    "i18next": "^23.8.2",
    "i18next-fs-backend": "^2.3.1",
    "surrealdb.node": "^0.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^2.8.4",
    "tsx": "^4.7.1",
    "typescript": "5.3.3"
  }
}
